#!/usr/bin/env ruby

if ENV['RESAMPLETEST']
  $LOAD_PATH << File.expand_path( File.dirname(__FILE__) + '/../lib' )
end
    
require File.dirname(__FILE__) + '/../lib/resample'
require File.dirname(__FILE__) + '/../lib/resample/gmdata'
require File.dirname(__FILE__) + '/../lib/resample/errors'
require File.dirname(__FILE__) + '/../lib/resample/cli'

GMResamplerProgramManager.new(ARGV).run
